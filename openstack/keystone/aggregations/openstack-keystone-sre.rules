groups:
- name: openstack-keystone-sre.rules
  rules:
  - record: sre:api_error_ratio_slo:percent
    labels:
        app: "keystone"
    expr: 1
  - record: sre:api_apdex_slo:percent
    labels:
        app: "keystone"
        api: "token-create"
    expr: 80
  - record: sre:api_apdex_slo:percent
    labels:
        app: "keystone"
        api: "token-validate"
    expr: 80

  - record: sre:api_errors_per_response:ratio_rate5m
    labels:
        app: "keystone"
    expr: |2
        sum(rate(nginx_ingress_controller_requests{ingress="keystone", status=~"5.."}[5m])) /  sum(rate(nginx_ingress_controller_requests{ingress="keystone"}[5m]))

  - record: sre:api_errors_per_response:ratio_rate30m
    labels:
        app: "keystone"
    expr: |2
        sum(rate(nginx_ingress_controller_requests{ingress="keystone", status=~"5.."}[30m])) /  sum(rate(nginx_ingress_controller_requests{ingress="keystone"}[30m]))

  - record: sre:api_errors_per_response:ratio_rate1h
    labels:
        app: "keystone"
    expr: |2
        sum(rate(nginx_ingress_controller_requests{ingress="keystone", status=~"5.."}[1h])) /  sum(rate(nginx_ingress_controller_requests{ingress="keystone"}[1h]))

  - record: sre:api_errors_per_response:ratio_rate2h
    labels:
        app: "keystone"
    expr: |2
        sum(rate(nginx_ingress_controller_requests{ingress="keystone", status=~"5.."}[2h])) /  sum(rate(nginx_ingress_controller_requests{ingress="keystone"}[2h]))

  - record: sre:api_errors_per_response:ratio_rate6h
    labels:
        app: "keystone"
    expr: |2
        sum(rate(nginx_ingress_controller_requests{ingress="keystone", status=~"5.."}[6h])) /  sum(rate(nginx_ingress_controller_requests{ingress="keystone"}[6h]))

  - record: sre:api_errors_per_response:ratio_rate1d
    labels:
        app: "keystone"
    expr: |2
        sum(rate(nginx_ingress_controller_requests{ingress="keystone", status=~"5.."}[1d])) /  sum(rate(nginx_ingress_controller_requests{ingress="keystone"}[1d]))

  - record: sre:api_errors_per_response:ratio_rate3d
    labels:
        app: "keystone"
    expr: |2
        sum(rate(nginx_ingress_controller_requests{ingress="keystone", status=~"5.."}[3d])) /  sum(rate(nginx_ingress_controller_requests{ingress="keystone"}[3d]))

  - record: sre:api_apdex_sli:rate5m
    labels:
        app: "keystone"
        api: "token-create"
    expr: |2
        (sum(rate(openstack_watcher_api_requests_duration_seconds_bucket{le="0.1", target_type_uri=~"^data/security/auth/tokens", action="authenticate"}[5m])) + sum(rate(openstack_watcher_api_requests_duration_seconds_bucket{le="1", target_type_uri=~"^data/security/auth/tokens", action=~"authenticate"}[5m]))) / 2 / sum(rate(openstack_watcher_api_requests_duration_seconds_bucket{le="+Inf", target_type_uri=~"^data/security/auth/tokens", action=~"authenticate"}[5m]))

  - record: sre:api_apdex_sli:rate30m
