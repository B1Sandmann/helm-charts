groups:
- name: interconnect_rules
  rules:
  - record: network:dc_interconnect_latency:milliseconds_rate5m
    expr: sum(rate(cloudprober_latency_sum{interconnect_type="dc",probed_to_type="master"}[5m]))/sum(rate(cloudprober_latency_count{interconnect_type="dc",probed_to_type="master"}[5m]))
  - record: network:region_interconnect_latency:milliseconds_rate5m
    expr: sum(rate(cloudprober_latency_sum{interconnect_type="region",probed_to_type="master"}[5m]))/sum(rate(cloudprober_latency_count{interconnect_type="region",probed_to_type="master"}[5m]))