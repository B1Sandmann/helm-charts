groups:
- name: apicduplicateips.alerts
  rules:
  - alert: ApicDuplicateIps
    expr: count(network_apic_duplicate_ip) > 0
    labels:
      severity: info
      tier: net
      service: apic
      dashboard: apic-health
    annotations:
      description: "{{ $value }} duplicate ips"
      summary: "{{ $value }} duplicate ips"
