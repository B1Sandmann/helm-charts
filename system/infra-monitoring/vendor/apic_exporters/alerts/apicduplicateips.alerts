groups:
- name: apicduplicateips.alerts
  rules:
  - alert: ApicDuplicateIps
    expr: count(network_apic_duplicate_ip)
    labels:
      severity: info
      tier: net
      service: apic
      dashboard: apic-health
      meta: "{{`{{$value}}`}} duplicate ips in {{ $labels.region }}"
    annotations:
      description: "{{`{{$value}}`}} duplicate ips in {{ $labels.region }}"
      summary: "{{`{{$value}}`}} duplicate ips in {{ $labels.region }}"
